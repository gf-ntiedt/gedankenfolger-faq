page {
  includeJSFooter {
    gedankenfolger_faq = EXT:gedankenfolger_faq/Resources/Public/JavaScript/faq.js
    gedankenfolger_faq.forceOnTop = 1
  }
  inlineSettings {
      gf_faq_rootname = {$faq.rootName}
  }
}

[{$faq.css.default}==1] && [{$faq.scss.default}==0]
page.includeCSS {
  gedankenfolger_faq = EXT:gedankenfolger_faq/Resources/Public/Css/faq.css
}
[global]

[{$faq.scss.default}==1]
page.includeCSS {
  gedankenfolger_faq = EXT:gedankenfolger_faq/Resources/Public/Scss/faq.scss
  gedankenfolger_faq.outputfile = EXT:gedankenfolger_faq/Resources/Public/Css/faq.css
}
[global]